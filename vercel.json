{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/create_image?api_key=${API_KEY}",
            "schedule": "0 8 * * *"
        },
        {
            "path": "/post_image?api_key=${API_KEY}",
            "schedule": "30 8 * * *"
        }
    ]
}